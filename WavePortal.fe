use std::context::Context


contract WavePortal {
      
   totalWaves: Map<address,u256>
  

    pub fn wave(self,ctx:Context) {
       self.totalWaves[ctx.msg_sender()] +=1;
      
    }

    pub fn getTotalWaves(self, addr: address)-> u256{
        
        return self.totalWaves[addr].to_mem()
    }

   
}